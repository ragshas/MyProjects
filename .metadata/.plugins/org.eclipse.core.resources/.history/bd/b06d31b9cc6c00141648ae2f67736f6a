package com.example.appfootball;

public class RssItem {
	
	//Item title
	private String title;
	//Item Link
	private String link;
	
	private String getTitle(){
		return title;
	}
	private void setTitle(String title){
		this.title = title;
	}
	private String getLink(){
		return link;
	}
	private void setLink(String link){
		this.link = link;
	}
	

}
